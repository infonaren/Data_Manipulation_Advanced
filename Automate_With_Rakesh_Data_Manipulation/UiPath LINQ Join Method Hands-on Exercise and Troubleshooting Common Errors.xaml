            </ui:AssignOperation.To>
            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="sd:DataTable" xml:space="preserve">[(
From a In dt_Employees
Join b In dt_Department
On a("DeptID").ToString.Trim Equals  b("DeptID").ToString.Trim
Select out_DT1.Rows.Add({a("ID"),a("Name"),a("DeptID"),a("Salary"),b("DeptName")})
).CopyToDataTable]</InArgument>
            </ui:AssignOperation.Value>
          </ui:AssignOperation>
          <ui:AssignOperation sap2010:WorkflowViewState.IdRef="AssignOperation_2">
            <ui:AssignOperation.To>
