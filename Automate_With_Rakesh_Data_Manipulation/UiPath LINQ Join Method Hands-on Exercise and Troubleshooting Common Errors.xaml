            </ui:AssignOperation.To>
            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="sd:DataTable" xml:space="preserve">[dt_Employees.AsEnumerable.Join(dt_Department.AsEnumerable,
Function(emp) emp("DeptID").ToString,
Function(dep) dep("DeptID").ToString,
Function(emp,dep) New Object() {
emp("ID").ToString,
emp("Name").ToString,
emp("DeptID").ToString,
dep("DeptName").ToString,
emp("Salary").ToString
}
).Select(Function(x) out_DT2.Rows.Add(x)).CopyToDataTable]</InArgument>
            </ui:AssignOperation.Value>
          </ui:AssignOperation>
        </scg:List>
      </ui:MultipleAssign.AssignOperations>
